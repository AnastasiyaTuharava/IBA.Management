<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="AddNewProjectPane" prefHeight="670.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="by.iba.management.view.fxml.AddNewProjectController">
    <children>
        <TitledPane fx:id="AddNewProject" animated="false" prefHeight="670.0" prefWidth="1100.0" text="Add New Project">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <Label layoutX="38.0" layoutY="64.0" text="Project Name: " />
                        <Label layoutX="38.0" layoutY="114.0" text="Project Description: " />
                        <TextField fx:id="projectDescription" alignment="TOP_LEFT" layoutX="247.0" layoutY="109.0" prefHeight="262.0" prefWidth="668.0" />
                        <TextField fx:id="projectName" layoutX="247.0" layoutY="58.0" />
                        <Button fx:id="fxSaveButton" layoutX="1007.0" layoutY="557.0" mnemonicParsing="false" onAction="#handleSaveProject" text="Save" />
                        <Button fx:id="fxCancelButton" layoutX="885.0" layoutY="557.0" mnemonicParsing="false" onAction="#backToProjectsList" text="Cancel" />
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </children>
</AnchorPane>
