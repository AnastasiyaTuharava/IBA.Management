<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="ProjectProfile" prefHeight="670.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="by.iba.management.view.fxml.ProjectProfileController">
   <children>
      <TitledPane fx:id="fxProjectProfile" animated="false" prefHeight="670.0" prefWidth="1100.0" text="Project Profile">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <Label layoutX="38.0" layoutY="100.0" text="Project Name: " />
                  <Label layoutX="38.0" layoutY="150.0" text="Project Description: " />
                  <Label layoutX="38.0" layoutY="50.0" text="Project ID: " />
                  <TextField fx:id="projectDescription" layoutX="247.0" layoutY="145.0" prefHeight="161.0" prefWidth="689.0" />
                  <TextField fx:id="projectName" layoutX="247.0" layoutY="94.0" />
                  <TextField fx:id="projectId" layoutX="247.0" layoutY="44.0" />
                  <Button fx:id="fxSaveProjectButton" layoutX="870.0" layoutY="560.0" mnemonicParsing="false" text="Save" />
