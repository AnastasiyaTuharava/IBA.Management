                      <TableColumn fx:id="employeeId" prefWidth="50.0" text="ID" />
                        <TableColumn fx:id="firstName" prefWidth="180.0" text="First Name" />
                      <TableColumn fx:id="lastName" prefWidth="230.0" text="Last Name" />
                    </columns>
                  </TableView>
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
   </children>
</AnchorPane>
